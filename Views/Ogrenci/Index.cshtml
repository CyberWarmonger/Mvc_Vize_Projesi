@using Mvc_Vize_Projesi.Models.Entity;
@model List<Tbl_Ogrenci>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}
<table class="table table-bordered">
    <tr>
        <th>Ogrendi Id</th>
        <th>Ogrendi Ad Soyad</th>
        <th>Ogrendi No</th>
        <th>Ogrendi Kredi</th>
        <th>Mezun</th>
        <th>Sil</th>
        <th>Guncelle</th>
    </tr>
    <tbody>
        @foreach(var ogr in Model)
            {
                <tr>
                    <td>@ogr.ogreinciid</td>
                    <td>@ogr.OgrecnciAdSoyad</td>
                    <td>@ogr.OgrenciNo</td>
                    <td>@ogr.OgrenciKredi</td>
                    <td>@ogr.OgrenciMezun</td>
                    <td><a href="/Ogrenci/Sil/@ogr.ogreinciid" class="btn btn-danger">Sil</a></td>
                    <td><a href="/Ogrenci/Guncelle/@ogr.ogreinciid" class="btn btn-success">Guncelle</a></td>
                </tr>
            }
    </tbody>
</table>
<a href="/Ogrenci/YeniOgrenci/" class="btn btn-primary">Yeni Ogrenci Ekle</a>